@model OOTTracker.Models.ItemCheckTypes.ItemCheckTypeEditViewModel
@{
    Layout = "_Layout";
}
<div class="header">
    <h1 class="h4">Edit Item Check Type</h1>
</div>
<form method="post">
    <div class="row mb-3">
        <div class="col-md-6">
            <label asp-for="Name">Name</label>
            <input class="form-control" asp-for="Name" />
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            <div class="d-flex justify-content-between">
                <button type="submit" class="btn btn-primary">Save Changes</button>
                <a asp-action="Index" class="btn btn-secondary">Cancel</a>
            </div>
        </div>
    </div>
</form>
